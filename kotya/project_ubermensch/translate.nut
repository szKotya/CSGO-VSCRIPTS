enum Enum_LANGUAGE
{
	ENG = 0,
	RU = 1,
}

function SetLang(ID)
{
	if (Enum_LANGUAGE.RU == ID)
	{
		Translate = TranslateRU
	}
	else if (Enum_LANGUAGE.ENG == ID)
	{
		Translate = TranslateENG
	}
}